<configure 
    xmlns="http://namespaces.zope.org/zope"
    xmlns:plone="http://namespaces.plone.org/plone"
    i18n_domain="edeposit.content">
  <!-- Ensure Plone's portlets ZCML has already been processed; without
       this, we may get a permission lookup error
  -->
  <include package="plone.app.portlets" />
  <!-- -*- extra stuff goes here -*- -->

  <plone:portlet
      name="edeposit.content.portlets.AddOriginalFile"
      interface=".addoriginalfile.IAddOriginalFile"
      assignment=".addoriginalfile.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".addoriginalfile.Renderer"
      addview=".addoriginalfile.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.AuthorsPortlet"
      interface=".authorsportlet.IAuthorsPortlet"
      assignment=".authorsportlet.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".authorsportlet.Renderer"
      addview=".authorsportlet.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.OriginalFilesLinks"
      interface=".originalfileslinks.IOriginalFilesLinks"
      assignment=".originalfileslinks.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".originalfileslinks.Renderer"
      addview=".originalfileslinks.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.TechnicalMetadata"
      interface=".technicalmetadata.ITechnicalMetadata"
      assignment=".technicalmetadata.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".technicalmetadata.Renderer"
      addview=".technicalmetadata.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.ChooseProperAlephRecord"
      interface=".chooseproperalephrecord.IChooseProperAlephRecord"
      assignment=".chooseproperalephrecord.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".chooseproperalephrecord.Renderer"
      addview=".chooseproperalephrecord.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.OriginalFileExternalLinks"
      interface=".originalfileexternallinks.IOriginalFileExternalLinks"
      assignment=".originalfileexternallinks.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".originalfileexternallinks.Renderer"
      addview=".originalfileexternallinks.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.ISBNSubjectValidation"
      interface=".isbnsubjectvalidation.IISBNSubjectValidation"
      assignment=".isbnsubjectvalidation.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".isbnsubjectvalidation.Renderer"
      addview=".isbnsubjectvalidation.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.WaitingForAcquisition"
      interface=".waitingforacquisition.IWaitingForAcquisition"
      assignment=".waitingforacquisition.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".waitingforacquisition.Renderer"
      addview=".waitingforacquisition.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.WaitingForAleph"
      interface=".waitingforaleph.IWaitingForAleph"
      assignment=".waitingforaleph.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".waitingforaleph.Renderer"
      addview=".waitingforaleph.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.ISBNGenerationPortlet"
      interface=".isbngenerationportlet.IISBNGenerationPortlet"
      assignment=".isbngenerationportlet.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".isbngenerationportlet.Renderer"
      addview=".isbngenerationportlet.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.ePublicationMetadataPortlet"
      interface=".epublicationmetadataportlet.IePublicationMetadataPortlet"
      assignment=".epublicationmetadataportlet.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".epublicationmetadataportlet.Renderer"
      addview=".epublicationmetadataportlet.AddForm"
      />


  <plone:portlet
      name="edeposit.content.portlets.ISBNGeneratingPortlet"
      interface=".isbngeneratingportlet.IISBNGeneratingPortlet"
      assignment=".isbngeneratingportlet.Assignment"
      view_permission="zope2.View"
      edit_permission="cmf.ManagePortal"
      renderer=".isbngeneratingportlet.Renderer"
      addview=".isbngeneratingportlet.AddForm"
      />

</configure>
