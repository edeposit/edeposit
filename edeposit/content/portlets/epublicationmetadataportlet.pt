<dl class="portlet portletePublicationMetadataPortlet"
    i18n:domain="edeposit.content">

  <dt class="portletHeader">
    <span class="portletTopLeft"></span>
    <span>
      Popisná metadata
    </span>
    <span class="portletTopRight"></span>
  </dt>
  
  <dd class="portletItem odd">
    <table>
      <tbody>
        <tr><th>Název</th><td tal:content="here/getParentTitle"></td></tr>
        <tr><th>Podnázev</th><td tal:content="here/getPodnazev">Podnazev</td></tr>
        <tr><th>Část</th><td tal:content="here/getCast">Cast</td></tr>
        <tr><th>Nakladavatel/vydavatel</th><td tal:content="here/getNakladatelVydavatel">Nakladatel/vydavatel</td></tr>
      </tbody>
    </table>
    
    <table>
      <tbody>
        <tr><th>ISBN</th><td tal:content="here/isbn"></td></tr>
      </tbody>
    </table>
    
    </dd><dd class="portletFooter"></dd>
</dl>
